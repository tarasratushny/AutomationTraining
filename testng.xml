<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Automation Training">
  <listeners>
    <listener class-name="com.miamato.TestResultsListener" />
    <listener class-name="com.miamato.TestReporter" />
  </listeners>
  <test name="Allure Reports Example Edge">
    <parameter name="browserName" value="Edge" />
    <classes>
      <class name="com.miamato.test.SteamTest" />
    </classes>
  </test>
  <test name="Allure Reports Example Chrome">
    <parameter name="browserName" value="Chrome" />
    <classes>
      <class name="com.miamato.test.SteamTest" />
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->